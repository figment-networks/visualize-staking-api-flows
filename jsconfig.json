{
  "compilerOptions": {
    "jsx": "react",
    "target": "esnext",
    "module": "esnext",
    "checkJs": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@images/*": ["public/img/*"],
      "@styles/*": ["styles/*"],
      "@components/*": ["components/*"],
      "@utilities/*": ["utilities/*"]
    }
  },
  "include": ["**/*.js", "**/*.jsx", "utilities/*.js"],
  "exclude": ["node_modules"]
}
